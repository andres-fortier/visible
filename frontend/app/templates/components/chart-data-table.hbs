<table class="table table-bordered table-striped">
  <thead>
    <tr>
      <th>Date</th>
      <th>Open</th>
      <th>Close</th>
      <th>High</th>
      <th>Low</th>
      <th>Volume</th>
      <th class="actions-column">Actions</th>
    </tr>
  </thead>
  <tbody>
    {{new-chart-data-point}}
    {{#each tableData as |stockQuote|}}
      {{chart-data-table-row stockQuote=stockQuote}}
    {{/each}}
  </tbody>
</table>

<div class="row">
  <div class="col-xs-12">
    <div class="panel panel-default">
      <div class="panel-heading">YHOO stock price</div>
      <div class="panel-body">
        <div class="row">
          <div class="col-xs-12">
            <!-- {{input type="checkbox" checked=showClose}} Show close -->
            {{#each chartSeries as |series index|}}
              <button {{action "toggleShowClose" index}}>Toggle {{series.name}}</button>
            {{/each}}
          </div>
        </div>
        <div class="row">
          <div class="col-xs-12">
            {{high-charts mode=chartMode content=chartData chartOptions=chartOptions theme=theme}}
          </div>
        </div>
      </div>
    </div>
  </div>
</div>


<div class="row">
  <div class="col-xs-12">
    <div class="panel panel-default">
      <div class="panel-heading">YHOO</div>
      <div class="panel-body">
        {{models-table data=tableData columns=tableColumns changeValue='changeValue' startEditing='startEditing'}}
      </div>
    </div>
  </div>
</div>
